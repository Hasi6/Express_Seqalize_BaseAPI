{
  "name": "images-api",
  "version": "1.0.0",
  "description": "api",
  "main": "index.js",
  "scripts": {
    "start": "nodemon ./src/index.ts",
    "start:dev": "cross-env NODE_ENV=development yarn build && node dist/index.js",
    "start:prod": "cross-env NODE_ENV=production yarn build && node dist/index.js",
    "build": "webpack --progress",
    "seed": "ts-node ./src/seeds.ts",
    "build:serve": "yarn build && node dist/index.js",
    "lint": "eslint . --ext .ts",
    "docker:build-dev": "docker build -t my-api:dev .",
    "docker:run-dev": "docker run -itd -p 4000:4000 my-api:dev",
    "docker:dev": "yarn docker:build-dev && yarn docker:run-dev",
    "docker:rm-containers": "docker stop $(docker ps -a -q)",
    "docker:rm-images": "docker system prune -a"
  },
  "author": "Hasitha",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "7.16.11",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/morgan": "1.9.3",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "5.8.1",
    "@typescript-eslint/parser": "5.8.1",
    "babel-plugin-module-resolver": "4.1.0",
    "copy-webpack-plugin": "11.0.0",
    "dotenv-webpack": "8.0.0",
    "eslint": "8.5.0",
    "nodemon": "^2.0.20",
    "ts-loader": "9.3.1",
    "ts-node": "10.9.1",
    "ts-node-dev": "1.1.8",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.5.4",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-node-externals": "3.0.0"
  },
  "dependencies": {
    "clean-webpack-plugin": "4.0.0",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "dotenv": "10.0.0",
    "express": "4.17.2",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.14.2",
    "fs": "^0.0.1-security",
    "helmet": "^6.0.0",
    "module-alias": "^2.2.2",
    "morgan": "1.10.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.28.0",
    "sequelize-typescript": "^2.1.5",
    "sqlite3": "^5.1.4",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "zod": "^3.20.2"
  },
  "_moduleAliases": {
    "@services": "src/services",
    "@controllers": "src/controllers",
    "@data": "src/data",
    "@middlewares": "src/middlewares",
    "@routes": "src/routes",
    "@mixins": "src/mixins",
    "@utils": "src/utils"
  }
}
